{
    "elasticsearch": {
        "netobserv-perf": [
            {
                "filter": {
                    "metric_name.keyword": "nFlowsProcessedPerMinute"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        {
                            "percentiles": {
                                "percents": [
                                    90
                                ]
                            }
                        },
                        "avg"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "nPacketsProcessedPerMinute"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        {
                            "percentiles": {
                                "percents": [
                                    90
                                ]
                            }
                        },
                        "avg"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "lokiStorageUsage"
                },
                "buckets": [
                    "metadata.persistentvolumeclaim.keyword"
                ],
                "aggregations": {
                    "value": [
                        "max"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "FlowProcessingTime"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        {
                            "percentiles": {
                                "percents": [
                                    90
                                ]
                            }
                        },
                        "avg"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "cpuUsage"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        "avg"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "memoryUsage"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        "avg"
                    ]
                }
            }
        ]
    }
}
