{
  "elasticsearch": {
    "k8s-netperf": [
      {
        "filter": {
          "profile": "TCP_STREAM"
        },
        "buckets": [
          "hostNetwork",
          "messageSize",
          "samples",
          "parallelism",
          "acrossAZ",
          "service"
