{
  "elasticsearch": {
    "ripsaw-kube-burner": [
      {
        "filter": {
          "metricName.keyword": "nodeCPU-AggregatedWorkers"
        },
        "buckets": [
          "labels.mode.keyword"
        ],
        "aggregations": {
          "value": [
            "avg",
            "max"
          ]
        }
      },
      {
        "filter": {
          "metricName.keyword": "nodeMemoryUtilization-AggregatedWorkers"
        },
        "buckets": [
          "jobConfig.name.keyword"
        ],
        "aggregations": {
          "value": [
            "avg",
            "max"
          ]
        }
      }
    ]
  }
}